var win=Ti.UI.currentWindow;var b1=Ti.UI.createButton({title:'Fire Event 1',width:200,height:40,top:10});b1.addEventListener('click',function()
{Ti.App.fireEvent('event_one',{data:['1','2','3']});});win.add(b1);var b2=Ti.UI.createButton({title:'Fire Event 2',width:200,height:40,top:60});b2.addEventListener('click',function()
{Ti.App.fireEvent('event_two',{name:'Foo',city:'Palo Alto'});});win.add(b2);var b3=Ti.UI.createButton({title:'Fire Event 3',width:200,height:40,top:110});b3.addEventListener('click',function()
{b2.fireEvent('click');});win.add(b3);